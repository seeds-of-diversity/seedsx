THEMES_DIR=$SITES_DIR/themes


echo Unarchive themes/bootstrap to swww8/themes
echo Unarchive themes/breeze to swww8/themes


#rm -Rf $MODULES_DIR/seeds
cp -R modules/seeds $MODULES_DIR 

#rm -Rf $THEMES_DIR/seedbreeze2017
cp -R themes/seedbreeze2017 $THEMES_DIR

